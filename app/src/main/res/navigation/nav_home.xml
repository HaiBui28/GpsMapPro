<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_host_home"
    app:startDestination="@id/cameraFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.tapbi.spark.gpsmappro.ui.main.home.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/cameraFragment"
        android:name="com.tapbi.spark.gpsmappro.ui.main.camera.CameraFragment"
        android:label="CameraFragment" >
        <action
            android:id="@+id/action_cameraFragment_to_googleMapFragment"
            app:destination="@id/googleMapFragment" />
    </fragment>
    <fragment
        android:id="@+id/googleMapFragment"
        android:name="com.tapbi.spark.gpsmappro.ui.main.googlemap.GoogleMapFragment"
        android:label="GoogleMapFragment" />
</navigation>